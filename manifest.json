{
  "manifest_version": 2,

  "name": "Boostano",
  "description": "Seo Increase Extension",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["shared/jquery.min.js","background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["management","tabs", "storage", "http://*/", "https://*/"]
}
